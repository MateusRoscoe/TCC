{
  "stages": [
    { "duration": "10s", "target": 1000 },
    { "duration": "1h", "target": 20000 }
  ],
  "thresholds": {
    "http_req_duration": [
      {
        "threshold": "p(90)<1000",
        "abortOnFail": true,
        "delayAbortEval": "10s"
      }
    ],
    "errors": [
      { "threshold": "count<10", "abortOnFail": true, "delayAbortEval": "10s" }
    ]
  },
  "discardResponseBodies": true,
  "tags": {
    "name": "breakpoint"
  }
}
